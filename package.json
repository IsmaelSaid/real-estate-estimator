  "scripts": {
    "dev": "dotenv -e .env.dev next dev",
    "prod": "dotenv -e .env.prod next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "seed:dev": "dotenv -e .env.dev -- prisma db seed",
