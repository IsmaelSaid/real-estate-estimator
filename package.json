{
  "name": "cda-project-2024",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "dotenv -e .env.dev next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "seed:dev": "dotenv -e .env.dev -- prisma db seed",
    "seed:prod": "dotenv -e .env.prod -- prisma db seed",
    "studio:dev": "dotenv -e .env.dev -- prisma studio",
    "studio:prod": "dotenv -e .env.prod -- prisma studio",
    "clear:dev": "dotenv -e .env.dev -- prisma db push --force-reset",
    "clear:prod": "dotenv -e .env.prod -- prisma db push --force-reset",
    "generate:prod": "dotenv -e .env.prod -- prisma generate",
    "generate:dev": "dotenv -e .env.dev -- prisma generate",
    "test": "dotenv -e .env.dev -- jest -i --detectOpenHandles"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.19",
    "@mui/material": "^5.15.19",
    "@prisma/client": "^5.15.0",
    "@types/bcrypt": "^5.0.2",
    "dotenv-cli": "^7.4.2",
    "next": "14.2.3",
    "next-auth": "^4.24.7",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^16.0.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "bcrypt": "^5.1.1",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8",
    "prisma": "^5.15.0",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
